{
  "manifest_version": 3,
  "name": "Akıllı Eksper",
  "description": "Araç detay sayfasında chatbot ile interaktif araç değerlendirme.",
  "version": "2.0",
  "permissions": ["scripting", "activeTab", "tabs", "storage"],
  "host_permissions": ["*://*.sahibinden.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.sahibinden.com/*"],
      "js": ["content.js"]
    }
  ]
}
